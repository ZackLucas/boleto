{
  "requiresCompatibilities": ["EC2"],
  "containerDefinitions": [
    {
      "name": "boleto-task",
      "essential": true,
      "memoryReservation": "",
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/boleto",
          "awslogs-region": "sa-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "cpu": 0,
      "memory": "128"
    }
  ],
  "family": "boleto",
  "networkMode": null,
  "memory": "128",
  "cpu": null,
  "taskRoleArn": null,
  "executionRoleArn": null
}
